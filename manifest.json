{
    "manifest_version": 3,
    "name": "Transcript Summariser for Youtube",
    "version": "1.0",
    "permissions": ["activeTab","declarativeContent"],
    "host_permissions":["http://127.0.0.1:5000/*"],
    "description": "This extension Summarises youtube videos with Transcript and gives user the crux of the video.",
    "icons": {
      "16": "icon.png",
      
      "48": "icon.png",
      "128": "icon.png"
    },
    "action": {
      "default_title": "Summarise the video",
      "default_icon": {
        "16": "icon.png",
        
        "48": "icon.png",
        "128": "icon.png"
      },
      
      "default_popup": "popup.html"
    }
    
    
  }
  